<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cis.myeventapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudentListActivity"></activity>
        <activity android:name=".add_student_to_event.AddStuToEventActivity" />
        <activity android:name=".ShowListStudentActivity" />
        <activity android:name=".AddTodoStudentActivity" />
        <activity android:name=".detailActivity" />
        <activity android:name=".EventActivity" />
        <activity android:name=".StudentActivity" />
        <activity android:name=".AddeventActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>